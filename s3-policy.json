{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "TerraformS3Buckets",
      "Effect": "Allow",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::terraform-state-production-252545",
        "arn:aws:s3:::terraform-state-production-252545/*"
      ]
    },
    {
      "Sid": "AllowListBuckets",
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    }
  ]
}

